{
  "name": "tabelline",
  "version": "1.0.0",
  "description": "A simple way to learn multiplication tables",
  "scripts": {
    "start": "concurrently --kill-others \"npm run watch\" \"npm run server\"",
    "server": "node ./cypress/static-server",
    "build": "gulp",
    "watch": "gulp watch",
    "cy:run": "cypress run --spec cypress/integration/*",
    "cy:open": "cypress open",
    "test": "gulp && start-server-and-test server http://localhost:9981/src/index.html cy:run"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.rainone.me:luca/tabelline.git"
  },
  "author": "Luca Rainone",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/preset-env": "^7.9.6",
    "@babel/runtime": "^7.10.1",
    "babelify": "^10.0.0",
    "browserify": "^16.5.1",
    "concurrently": "^5.2.0",
    "core-js": "^3.6.5",
    "cypress": "^4.7.0",
    "cypress-localstorage-commands": "^1.1.10",
    "gulp": "^4.0.2",
    "gulp-clean": "^0.4.0",
    "gulp-cli": "^2.2.1",
    "gulp-concat": "^2.6.1",
    "gulp-cssnano": "^2.1.3",
    "gulp-less": "^4.0.1",
    "gulp-replace": "^1.0.0",
    "gulp-uglify": "^3.0.2",
    "start-server-and-test": "^1.11.0",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0",
    "workbox-expiration": "^5.1.3",
    "workbox-precaching": "^5.1.3",
    "workbox-routing": "^5.1.3",
    "workbox-strategies": "^5.1.3",
    "workbox-sw": "^5.1.3"
  },
  "dependencies": {}
}
